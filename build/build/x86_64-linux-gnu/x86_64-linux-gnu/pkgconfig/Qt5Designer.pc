prefix=/usr
exec_prefix=${prefix}
libdir=${prefix}/lib/x86_64-linux-gnu
includedir=${prefix}/include/qt5


Name: Qt5 Designer
Description: Qt Designer module
Version: 5.2.1
Libs: -L${libdir} -lQt5Designer 
